{
  "name": "qrcodecreater",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/index.js",
  "type": "module",
  "author": {
    "name": "LianQi",
    "url": "https://lianqidev.cn"
  },
  "description": "A sleek desktop application for creating QR codes.",
  "repository": {
    "type": "git",
    "url": "https://github.com/LianQi-Kevin/QRcodeCreater"
  },
  "license": "MIT",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "tsc --noEmit && vite build",
    "vite:preview": "vite preview",
    "electron:start": "electron --trace-warnings .",
    "electron:build": "tsc --noEmit && vite build && electron-builder"
  },
  "build": {
    "productName": "QRCodeCreator",
    "appId": "cn.lianqidev.QRCodeCreator",
    "directories": {
      "output": "build"
    },
    "win": {
      "icon": "public/icon.png",
      "target": [
        "portable"
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "compression": "maximum"
  },
  "files": [
    "dist/**/*",
    "!node_modules"
  ],
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.5.1",
    "qrcode": "^1.5.3",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/runtime-dom": "^3.4.15",
    "electron": "^28.1.4",
    "electron-builder": "^24.9.1",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-icons": "^0.18.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-inspect": "^0.8.1"
  }
}
