{
  "name": "qrcodecreater",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/index.js",
  "author": {
    "name": "LianQi",
    "url": "https://lianqidev.cn"
  },
  "description": "A sleek desktop application for creating QR codes.",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "vite build",
    "vite:preview": "vite preview",
    "electron:start": "electron --trace-warnings .",
    "electron:build": "vite build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "QRCodeCreator",
    "appId": "cn.lianqidev.QRCodeCreator",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        "portable"
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "compression": "maximum"
  },
  "files": [
    "dist/**/*",
    "!node_modules"
  ],
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.5.1",
    "qrcode": "^1.5.3",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.2",
    "electron": "^28.1.4",
    "electron-builder": "^24.9.1",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-icons": "^0.18.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.15.6",
    "vite-plugin-inspect": "^0.8.1"
  }
}
